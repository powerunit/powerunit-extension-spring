<project xmlns="http://maven.apache.org/DECORATION/1.3.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">
  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.3.1</version>
  </skin>
  <custom>
    <fluidoSkin>
      <googleSearch>
      </googleSearch>
      <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
      <gitHub>
        <projectId>powerunit/powerunit-extensions-spring</projectId>
        <ribbonOrientation>right</ribbonOrientation>
        <ribbonColor>black</ribbonColor>
      </gitHub>
    </fluidoSkin>
  </custom>
  <poweredBy>
    <logo name="PowerUnit" href="http://www.powerunit.ch/"
          img="http://www.powerunit.ch/powerunit/logo.png"/>
    <logo name="Travis-ci" href="https://travis-ci.org/powerunit/powerunit-extensions-spring"
          img="https://travis-ci.org/powerunit/powerunit-extensions-spring.svg?branch=master"/>
  </poweredBy>
  <body>
  	<head>
  		<link rel="icon" type="image/png" href="logo.png" />
  		<link rel="shortcut icon" type="image/png" href="logo.png" />
  	</head>
    <menu ref="reports" />
    <menu name="Linked projects">
      <item name="PowerUnit" href="http://www.powerunit.ch/powerunit"/>
    </menu>
  </body>
</project>